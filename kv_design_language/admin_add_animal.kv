<admin_add_animal_url_popup@Screen>:
    
    name: "admin_add_animal_url_popup"

    MDFloatLayout:
        md_bg_color: (171/255, 196/255, 212/255, 1)
        size_hint_x: 1
        size_hint_y: 1
        # radius: [10,10,10,10]
        # radius: [45,45,45,45]
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        # orientation: 'vertical'

        MDFloatLayout:
            md_bg_color: (171/255, 196/255, 212/255, 1)
            # md_bg_color: (255/255,233/255,234/255,1)
            radius: [10, 10, 10, 10]
            size_hint_x: 0.9
            size_hint_y: 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            # orientation: 'vertical'

            MDTextField:
                id: image_url
                # md_bg_color: (0/255,0/255,0/255,1)
                # md_bg_color: (255/255,233/255,234/255,1)
                hint_text: "URL:"
                text_color: 0/255,0/255,0/255,1
                size_hint_x: 0.8
                size_hint_y: 0.2
                multiline: True
                readonly: False
                pos_hint: {"center_x": 0.5, "center_y": 0.5}

            MDIconButton:
                icon: "arrow-right"
                # md_bg_color: (255/255,233/255,234/255,1)
                size_hint_x: 0.9
                size_hint_y: 0.1
                radius: [45,45,45,45]
                font_size_x: self.width * 1
                source: ''
                pos_hint: {"center_x": 0.5, "center_y": 0.3}
                # on_press: root.load_image()
                on_press: 
                    root.load_image()
                    app.root.current = "admin_add_animal"


