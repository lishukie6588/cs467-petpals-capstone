                    md_bg_color: (255/255,233/255,234/255,1)
                    line_color: (255/255,233/255,234/255,1)
                    size_hint_x: 0.95
                    size_hint_y: 0.185
                    radius: [10,10,10,10]
                    text: "Gender"
                    font_style: "Button"
                    pos_hint: {"center_x": 0.5, "center_y": 1 - .0925 - 3* .0125 - 2* .185}
                    on_release:
                        app.root.current = "admin_browse_animals_gender_popup"

                MDRectangleFlatButton:
                    id: availability
                    md_bg_color: (255/255,233/255,234/255,1)
                    line_color: (255/255,233/255,234/255,1)
                    size_hint_x: 0.95
                    size_hint_y: 0.185
                    radius: [10,10,10,10]
                    text: "Availability"
                    font_style: "Button"
                    pos_hint: {"center_x": 0.5, "center_y": 1 - .0925 - 4* .0125 - 3* .185}
                    on_release:
                        app.root.current = "admin_browse_animals_availability_popup"

                MDRectangleFlatButton:
