<admin_browse_animals_breed_popup@Screen>:
    
    name: "admin_browse_animals_breed_popup"

    MDFloatLayout:
        md_bg_color: (171/255, 196/255, 212/255, 1)
        size_hint_x: 1
        size_hint_y: 1
        # radius: [10,10,10,10]
        # radius: [45,45,45,45]
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        # orientation: 'vertical'

        MDFloatLayout:
            md_bg_color: (171/255, 196/255, 212/255, 1)
            radius: [10, 10, 10, 10]
            size_hint_x: 0.9
            size_hint_y: 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            # orientation: 'vertical'

            MDIconButton:
                icon: "arrow-right"
                size_hint_x: 1
                size_hint_y: 0.2
                radius: [45,45,45,45]
                font_size_x: self.width * 1
                source: ''
                pos_hint: {"center_x": 0.5, "center_y": 0.7}
                # on_press: root.load_image()
                on_release: 
                    root.next_breed()

            MDFlatButton:
                id: selection_breed
                line: 0
                text: "Breed"
                font_style: "Button"
                text_color: 0/255,0/255,0/255,1
                size_hint_x: 1
                size_hint_y: 0.2
                multiline: True
                readonly: False
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                on_release:
                    app.root.current = "admin_browse_animals_search_popup"

            MDIconButton:
                icon: "arrow-left"
    
                size_hint_x: 1
                size_hint_y: 0.2
                radius: [45,45,45,45]
                font_size_x: self.width * 1
                source: ''
                pos_hint: {"center_x": 0.5, "center_y": 0.3}
                # on_press: root.load_image()
                on_release: 
                    root.previous_breed()


