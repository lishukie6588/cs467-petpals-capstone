                    text: "Breed"
                    font_style: "Button"
                    pos_hint: {"center_x": 0.5, "center_y": 1 - .0925 - 2* .0125 - .185}
                    on_release:
                        app.root.current = "admin_browse_animals_breed_popup"


                MDRectangleFlatButton:
