<admin_browse_animals@Screen>:
 
    name: "admin_browse_animals"
    on_pre_enter:
        root.load_species_breeds()
        root.load_gender()
        root.load_availability()
        root.load_dispositions()
        root.search_main()
   
    MDFloatLayout:
        md_bg_color: (171/255, 196/255, 212/255, 1)
        size_hint_x: 1
        size_hint_y: 1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}

        MDFloatLayout:
            md_bg_color: (255/255,233/255,234/255,1)
            size_hint_x: 0.965
            size_hint_y: 0.965
            radius: [10,10,10,10]
            pos_hint: {"center_x": 0.5, "center_y": 0.5}

            MDIconButton:
                md_bg_color: 0/255,187/255,224/255,1
                size_hint_x: 0.95
                size_hint_y: 0.05
                icon: "magnify"
                radius: [10,10,10,10]
                pos_hint: {"center_x": 0.5, "center_y": 0.9625}
                on_press: 
                    app.root.current = "admin_browse_animals_search_popup"

